<?xml version="1.0" encoding="utf-8" ?>
<Project name="CORP Edition Addons">
    <Package name="teleportToPosition">
        <Container name="modules">

            <Key ID="STR_CORP_TELEPORT_TO_POSITION_DN">
                <Original>Teleport to position</Original>
                <French>Téléportation vers une position</French>
            </Key>

            <Key ID="STR_CORP_TELEPORT_TO_POSITION_DESC">
                <Original>Attaches an action to synchronized objects allowing players to teleport to the position specified in this module.</Original>
                <French>Attache une action aux objets synchronisés qui permet aux joueurs de se téléporter vers la position renseignée dans le module.</French>
            </Key>

        </Container>

        <Container name="ui">

            <Key ID="STR_CORP_TELEPORT_TO_POSITION_TELEPORT_CATEGORY">
              <Original>Teleportation settings</Original>
              <French>Paramètres de la téléportation</French>
            </Key>

            <!-- teleport position -->
            <Key ID="STR_CORP_TELEPORT_TO_POSITION_TELEPORT_POSITION_DN">
                <Original>Teleport position</Original>
                <French>Position de téléportation</French>
            </Key>

            <Key ID="STR_CORP_TELEPORT_TO_POSITION_TELEPORT_POSITION_TOOLTIP">
                <Original>ASL coordinates on which the player will be teleported.</Original>
                <French>Coordonnées ASL où sera téléporté le joueur.</French>
            </Key>

            <!-- teleport direction -->
            <Key ID="STR_CORP_TELEPORT_TO_POSITION_TELEPORT_DIRECTION_DN">
                <Original>Teleport direction</Original>
                <French>Direction de téléportation</French>
            </Key>

            <Key ID="STR_CORP_TELEPORT_TO_POSITION_TELEPORT_DIRECTION_TOOLTIP">
                <Original>The direction the player will face once teleported.</Original>
                <French>La direction vers laquelle le joueur fera face une fois téléporté.</French>
            </Key>

        </Container>
    </Package>
</Project>
